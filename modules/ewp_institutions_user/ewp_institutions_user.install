<?php

/**
 * @file
 * Install, update and uninstall functions for the EWP Institutions User bridge module.
 */

/**
 * Implements hook_install().
 */
function ewp_institutions_user_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function ewp_institutions_user_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
